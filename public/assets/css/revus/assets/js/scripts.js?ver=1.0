                        val = $(this).find("span"),
                        per = parseInt(val.text(), 10),
                        $this = $(this);
                    $this.waypoint(function () {
                        if (!$this.hasClass('animation-progress-complete')) {
                            $({
                                p: 0
                            }).animate({
                                p: per
                            }, {
                                duration: 1500,
                                step: function (p) {
                                    bar.css({
                                        transform: "rotate(" + (45 + (p * 1.8)) + "deg)"
                                    });
                                    val.text(p | 0);
                                }
                            }).delay(200);
                            $this.addClass('animation-progress-complete');
                        }
                    }, {
                        offset: offset
                    });

                });
